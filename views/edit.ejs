<% include partials/header %>

<div class="container">
	<div>
		<h1 style="text-align:center">
			Edit <%= campground.location %>
		</h1>
	</div>
	
	<div style="width:50% ;margin: 25px auto;">
		
		<form action="/landpage/<%= campground._id %>?_method=PUT" method="POST">
		
	<div class="form-group">
		<label>Location</label>
		<input  class="form-control"type="text" placeholder = "Name" name="camp[location]" value="<%=campground.location %>">
	
		
	</div>
	<div class="form-group">
		<label>image</label>
		<input  class="form-control"type="text" placeholder = "image url" name="camp[url]" value="<%=campground.url %>">
	
		
	</div>
	<div class="form-group">
		<label>About</label>
		<input  class="form-control"type="text" placeholder = "Name" name="camp[about]"  value="<%=campground.about %>">
	
		
	</div>
		<div class="form-group">
			<label>Description</label>
	
	<textarea class="form-control" name="camp[more]"><%=campground.more %></textarea>
	
	</div>
			
		<div class="form-group">
		
	<button class="btn btn-primary btn-block" >
		submit
	</button>
		
	</div>
		
		
	
</form>
		
	</div>
	
	

	
	
</div>






<% include partials/footer %>